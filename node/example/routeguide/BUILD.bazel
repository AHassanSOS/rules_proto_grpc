load("//node:library.bzl", "node_grpc_library")
load("@org_pubref_rules_node//node:rules.bzl", "node_module", "node_binary")

node_grpc_library(
    name = "routeguide",
    deps = ["//example/proto/routeguide:proto_library"],
)

node_module(
    name = "client_module",
    srcs = ["client.js"],
    deps = [
        ":routeguide",
    ],
)

node_binary(
    name = "client",
    entrypoint = ":client_module",
)
